{
	"inherits": ["avr"],
	"llvm-target": "avr-atmel-none",
	"cpu": "atmega328p",
	"build-tags": ["atmega328p", "atmega", "avr5"],
	"cflags": [
		"-mmcu=atmega328p"
	],
	"ldflags": [
		"-mmcu=avr5"
	],
	"linkerscript": "src/device/avr/atmega328p.ld",
	"extra-files": [
		"targets/avr.S",
		"src/device/avr/atmega328p.s"
	],
	"emulator": ["simavr", "-m", "atmega328p", "-f", "16000000"]
}
